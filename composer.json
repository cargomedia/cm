{
    "name": "cargomedia/cm",
    "license": "MIT",
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "composer/composer": "1.0.0-alpha8",
        "smarty/smarty": "3.1.16",
        "leafo/lessphp": "0.3.*",
        "easybook/geshi": "1.0.8.11",
        "amazonwebservices/aws-sdk-for-php": "1.5.*",
        "ruflin/Elastica": "v0.19.8.0",
        "phpmailer/phpmailer": "5.2.6",
        "michelf/php-markdown": "1.3-beta5",
        "lstrojny/functional-php": "1.0.0-alpha2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.10"
    },
    "autoload": {
        "psr-0": {
            "CM_": "library/",
            "CMService_": "library/",
            "CMTest_": "tests/helpers/CMTest/library/"
        }
    },
    "bin": ["bin/cm"],
    "extra": {
        "cm-modules": {
            "CM": {
                "path": ""
            }
        }
    }
}
