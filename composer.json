{
  "name": "cargomedia/cm",
  "license": "MIT",
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "php": ">=5.6.0",
    "composer/composer": "~1.2.0",
    "smarty/smarty": "3.1.16",
    "leafo/lessphp": "~0.5.0",
    "easybook/geshi": "1.0.8.11",
    "michelf/php-markdown": "1.4.0",
    "lstrojny/functional-php": "~1.2.4",
    "aws/aws-sdk-php": "~3.22",
    "nicmart/tree": "~0.2.0",
    "kissmetrics/kissmetrics-php": "~0.1.0",
    "elasticsearch/elasticsearch": "~2.0.0",
    "guzzlehttp/guzzle": "~6.0",
    "kickbox/kickbox": "~1.0.3",
    "tomaszdurka/codegenerator": "~0.5.0",
    "jenssegers/agent": "~2.3.1",
    "fluent/logger": "~1.0.1",
    "predis/predis": "~1.0.3",
    "danielstjules/stringy": "~2.3",
    "swiftmailer/swiftmailer": "~5.4.3",
    "ramsey/uuid": "~3.5.0",
    "mischiefcollective/colorjizz": "~1.0.0",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5.0",
    "tomaszdurka/mocka": "~0.12.3"
  },
  "autoload": {
    "psr-0": {
      "CM_": "library/",
      "CMService_": "library/"
    },
    "psr-4": {
      "CM\\": "library/CM/",
      "CMService\\": "library/CMService/"

    }
  },
  "autoload-dev": {
    "psr-0": {
      "CMTest_": "tests/helpers/CMTest/library/"
    },
    "psr-4": {
      "CM\\Test\\": "tests/library/CM/",
      "CMService\\Test\\": "tests/library/CMService/",
      "CMTest\\": "tests/helpers/CMTest/library/CMTest/"
    }
  },
  "bin": [
    "bin/cm",
    "bin/qunit"
  ],
  "extra": {
    "cm-modules": {
      "CM": {
        "path": ""
      }
    }
  }
}
